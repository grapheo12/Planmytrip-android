<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/flight_result"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/tvFlightId"
        android:layout_width="77dp"
        android:layout_height="25dp"
        android:background="#520A5A"
        android:text="flight_id"
        android:textColor="#FFFFFF"
        android:textColorHighlight="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:textColorLink="#FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_conversion_absoluteHeight="19dp"
        tools:layout_conversion_absoluteWidth="64dp" />

    <TextView
        android:id="@+id/tvAirlines"
        android:layout_width="71dp"
        android:layout_height="25dp"
        android:layout_marginStart="44dp"
        android:layout_marginLeft="44dp"
        android:background="#520A5A"
        android:text="airlines"
        android:textColor="#FFFFFF"
        android:textColorHighlight="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:textColorLink="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/tvFlightId"
        app:layout_constraintStart_toEndOf="@+id/tvFlightId"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_conversion_absoluteHeight="19dp"
        tools:layout_conversion_absoluteWidth="58dp" />

    <TextView
        android:id="@+id/tvDeparture"
        android:layout_width="71dp"
        android:layout_height="25dp"
        android:layout_marginStart="44dp"
        android:layout_marginLeft="44dp"
        android:background="#520A5A"
        android:text="departure"
        android:textColor="#FFFFFF"
        android:textColorHighlight="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:textColorLink="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/tvAirlines"
        app:layout_constraintStart_toEndOf="@+id/tvAirlines"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_conversion_absoluteHeight="19dp"
        tools:layout_conversion_absoluteWidth="61dp" />

    <TextView
        android:id="@+id/tvFare"
        android:layout_width="54dp"
        android:layout_height="25dp"
        android:layout_marginEnd="4dp"
        android:layout_marginRight="4dp"
        android:background="#520A5A"
        android:text="fare"
        android:textColor="#FFFFFF"
        android:textColorHighlight="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:textColorLink="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/tvDeparture"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_conversion_absoluteHeight="19dp"
        tools:layout_conversion_absoluteWidth="43dp" />

    <TextView
        android:id="@+id/tvSeatType"
        android:layout_width="71dp"
        android:layout_height="25dp"
        android:layout_marginStart="44dp"
        android:layout_marginLeft="44dp"
        android:background="#520A5A"
        android:text="seat_type"
        android:textColor="#FFFFFF"
        android:textColorHighlight="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:textColorLink="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/tvSeats"
        app:layout_constraintStart_toEndOf="@+id/tvSeats"
        app:layout_constraintTop_toBottomOf="@+id/tvAirlines"
        app:layout_constraintVertical_bias="0.0"
        tools:layout_conversion_absoluteHeight="19dp"
        tools:layout_conversion_absoluteWidth="76dp" />

    <TextView
        android:id="@+id/tvSeats"
        android:layout_width="77dp"
        android:layout_height="24dp"
        android:background="#520A5A"
        android:text="seats"
        android:textColor="#FFFFFF"
        android:textColorHighlight="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:textColorLink="#FFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvFlightId"
        tools:layout_conversion_absoluteHeight="19dp"
        tools:layout_conversion_absoluteWidth="46dp" />

    <EditText
        android:id="@+id/tvNop"
        android:layout_width="71dp"
        android:layout_height="27dp"
        android:background="#520A5A"
        android:ems="10"
        android:hint="Passengers"
        android:text="Passengers"
        android:textColor="#FFFFFF"
        android:textColorHighlight="#FFFFFF"
        android:textColorHint="#FFFFFF"
        android:textSize="14sp"
        android:inputType="number"
        app:layout_constraintBottom_toBottomOf="@+id/tvSeatType"
        app:layout_constraintEnd_toStartOf="@+id/book1"
        app:layout_constraintHorizontal_bias="0.733"
        app:layout_constraintStart_toEndOf="@+id/tvSeatType"
        app:layout_constraintTop_toBottomOf="@+id/tvDeparture"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/book1"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:text="Book"
        app:layout_constraintBottom_toBottomOf="@+id/tvNop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tvFare"
        app:layout_constraintVertical_bias="0.642" />


</androidx.constraintlayout.widget.ConstraintLayout>